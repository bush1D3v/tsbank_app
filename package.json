    "tailwindcss-animated": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.33.2",
    "postcss-import": "^16.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.3"
  },
