{
  "expo": {
    "name": "tsbank_app",
    "slug": "tsbank_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "scheme": "tsbank_app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./public/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
